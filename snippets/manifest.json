{
	"Create the <name> tag": {
		"prefix": "name",
		"body": [
			"<name>$1</name>"
		],
		"description": "Create the <name></name> tag for the metadata type name."
	},
	"Create the <members> tag": {
		"prefix": "members",
		"body": [
			"<members>$1</members>"
		],
		"description": "Create the <members></members> tag for the metadata type name."
	},
	"Create the <types>  structure": {
		"prefix": "types",
		"body": [
			"<types>",
			"\t<members>$1</members>",
			"\t<name>$2</name>",
			"</types>"
		],
		"description": "Create the <types>...</types> tag for the metadata type name."
	},
	"Create the <package> file structure": {
		"prefix": "package",
		"body": [
			"<?xml version=\"1.0\" encoding=\"UTF-8\" standalone=\"yes\"?>",
			"<Package xmlns=\"http://soap.sforce.com/2006/04/metadata\">",
			"",
			"\t<types>",
			"\t\t<members>$1</members>",
			"\t\t<name>$2</name>",
			"\t</types>",
			"",
			"\t<version>$3</version>",
			"</Package>"
		],
		"description": "Create the <Package> file structure of the XML file."
	},
